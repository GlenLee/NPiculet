<%@ Master Language="C#" AutoEventWireup="true" CodeFile="ContentPage.master.cs" Inherits="web_ContentPage" %>
<%@ Register src="uc/WebQuote.ascx" tagname="WebQuote" tagprefix="uc1" %>
<!DOCTYPE html>
<html>
<head>
