<%@ Page Title="" Language="C#" MasterPageFile="~/IndexPage.master" AutoEventWireup="true" CodeFile="Default.aspx.cs" Inherits="_Default" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="Server">
	<style type="text/css">
		@font-face { font-family: webfont; src: url(data:font/woff;charset=utf-8;base64,) format('woff'); }
		@font-face { font-family: macondo; src: url(data:font/woff;charset=utf-8;base64,); }
		html, body { margin: 0; padding: 0;width: 100%;height: 100%; color: #000; font-family: Verdana, STKaiti, sans-serif; }
		a, a:hover { color: #000; }

		.ui.grid  { font-size: 0; }
		.ui.grid .column { display: inline-block; width: 50%; }

		.ui-header { padding: 50px 50px 0 50px; }
		.ui-header a { text-decoration: none; }
		.ui-header .web-title { font-size: 32px; font-family: macondo, webfont; color: #000; }
		
		.logo { display: block; vertical-align: middle; margin: 0 auto 10px auto; border: 2px solid #f7f7f7; border-radius: 128px; }

		.home .title { display: block; margin: 30px 0 0 0; font-size: 64px; font-family: macondo, webfont; text-align: center; }
		.home .title span { font-size: 18px; padding-left:20px; color: #aaa; }

		.project { margin-top: 60px; text-align: center; }
		.project .title { margin: 10px; font-size: 32px; font-family: macondo, webfont; }
		.project .description { margin: 10px; font-size: 24px; font-family: macondo, webfont; color: #aaa; }
		.project .github { display: inline-block; margin: 0 8px; vertical-align: middle; }
		.project .demo { display: inline-block; margin: 0 8px; height: 32px; line-height: 32px; font-size: 12px; vertical-align: middle; }

		.about { padding: 20px 45px; }
		.copyright { font-family: macondo, "微软雅黑", "黑体", arial; text-align: center; font-size: 16px; }
		.copyright .contact { padding-top: 80px; }
		.copyright .beian { padding-top: 10px; }

		@media(max-width: 640px) {
			.ui-header { padding: 30px 30px 0 30px; }
			.logo { width: 50%; max-width: 256px; }
			.home .title { font-size: 42px; }
			.project { margin-top: 40px; }
			.project .title { font-size: 24px; }
			.project .description { font-size: 16px; }
			.project .github { margin: 20px; }
			.copyright { font-size: 12px; }
			.copyright .contact { padding-top: 30px; }
		}
	</style>
</asp:Content>

